{
  "name": "ts-template-todos",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run db:down && pnpm run db:up && vite dev --port 3000",
    "build": "pnpm run format && vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "oxlint --fix-suggestions",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt",
    "check": "oxfmt --check && oxlint --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio --port 4982",
    "db:up": "podman-compose up -d",
    "db:down": "podman-compose down",
    "db:restart": "podman-compose restart",
    "db:logs": "podman-compose logs -f db",
    "db:reset": "podman-compose down -v && podman-compose up -d"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@fontsource-variable/inter": "^5.2.8",
    "@tailwindcss/vite": "^4.1.17",
